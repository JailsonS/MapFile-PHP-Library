    "require-dev": {
        "phpunit/phpunit": "^7.3 || ^8.0",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "MapFile\\": "src/"
        },
        "exclude-from-classmap": [
