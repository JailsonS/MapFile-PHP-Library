{
    "name": "jbelien/mapfile-php-library",
    "type": "library",
    "description": "PHP Library to read/write MapServer mapfiles",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Jonathan BeliÃ«n"
        }
    ],
    "require": {
        "php": "^7.1",
        "doctrine/collections": "^1.5"
